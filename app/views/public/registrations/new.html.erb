<form action="<%= customer_registration_path %>" method="post">
  <%= csrf_meta_tags %>
  <div class="form-group row mx-auto">
    <div class="col-sm-1"></div>
    <label for="name" class="col-sm-1 col-form-label">名前</label>
    <div class="col-sm-1"></div>
    <label for="inputlast_name" class="col-sm-1 col-form-label">(姓)</label>
    <div class="col-sm-2">
      <input type="text" class="form-control" id="inputlast_name" name="customer[last_name]" required>
    </div>
    <label for="inputfirst_name" class="col-sm-1 col-form-label">(名)</label>
    <div class="col-sm-2">
      <input type="text" class="form-control" id="inputfirst_name" name="customer[first_name]" required>
    </div>
  </div>
  <div class="form-group row mx-auto">
    <div class="col-sm-1"></div>
    <label for="name_kana" class="col-sm-1 col-form-label">フリガナ</label>
    <div class="col-sm-1"></div>
    <label for="inputlast_name_kana" class="col-sm-1 col-form-label">(セイ)</label>
    <div class="col-sm-2">
      <input type="text" class="form-control" id="inputlast_name_kana" name="customer[last_name_kana]" required>
    </div>
    <label for="inputfirst_name_kana" class="col-sm-1 col-form-label">(メイ)</label>
    <div class="col-sm-2">
      <input type="text" class="form-control" id="inputfirst_name_kana" name="customer[first_name_kana]" required>
    </div>
  </div>
  <div class="form-group row mx-auto">
    <div class="col-sm-1"></div>
    <label for="inputemail" class="col-sm-1 col-form-label">メールアドレス</label>
    <div class="col-sm-2"></div>
    <div class="col-sm-2">
      <input type="email" class="form-control" id="inputemail" name="customer[email]" required>
    </div>
  </div>
  <div class="form-group row mx-auto">
    <div class="col-sm-1"></div>
    <label for="inputpostal_code" class="col-sm-2 col-form-label">郵便番号(ハイフンなし)</label>
    <div class="col-sm-1"></div>
    <div class="col-sm-2">
      <input type="text" class="form-control" id="inputpostal_code" name="customer[postal_code]" required>
    </div>
  </div>
  <div class="form-group row mx-auto">
    <div class="col-sm-1"></div>
    <label for="inputaddress" class="col-sm-2 col-form-label">住所</label>
    <div class="col-sm-1"></div>
    <div class="col-sm-5">
      <input type="text" class="form-control" id="inputaddress" name="customer[address]" required>
    </div>
  </div>
  <div class="form-group row mx-auto">
    <div class="col-sm-1"></div>
    <label for="inputpassword" class="col-sm-2 col-form-label">パスワード</label>
    <div class="col-sm-1"></div>
    <div class="col-sm-2">
      <input type="password" class="form-control" id="inputpassword" name="customer[password]" required>
    </div>
  </div>
  <div class="form-group row mx-auto">
    <div class="col-sm-1"></div>
    <label for="inputpassword_confirmation" class="col-sm-2 col-form-label">パスワードの確認</label>
    <div class="col-sm-1"></div>
    <div class="col-sm-2">
      <input type="password" class="form-control" id="inputpassword_confirmation" name="customer[password_confirmation]" required>
    </div>
  </div>
  <div class="form-group row mx-auto">
    <div class="col-sm-4"></div>
    <div class="offset-sm-2 col-sm-2">
      <button type="submit" class="btn btn-primary">新規登録</button>
    </div>
  </div>
</form>
<div class="container">
  <div class="row">
    <h3 class="mb-5 mt-5 px-4" style="background-color: orange;">既に登録済みの方</h3>
  </div>
  <div class="row">
    <div class="col">
      <%= render "public/shared/links" %><span>からログインしてください。</span>
    </div>
  </div>
</div>
