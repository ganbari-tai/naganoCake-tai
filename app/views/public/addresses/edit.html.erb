<h2>配送先編集</h2>
<div class="form-group">
  <%= form_with model: @address, url: address_path, method: :patch do |f|%>
    <div class="form-group">
      <%= f.label :postal_code, '郵便番号(ハイフンなし)' %>
      <%= f.number_field :postal_code %>
    </div>
    <div class="form-group">
      <%= f.label :address, '住所' %>
      <%= f.text_field :address %>
    </div>
    <div class="form-group">
      <%= f.label :name, '宛名' %>
      <%= f.text_field :name %>
    </div>
      <%= f.submit '変更内容を保存', class:"btn btn-success" %>
  <% end %>
</div>